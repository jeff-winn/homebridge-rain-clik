{
  "compilerOptions": {  
    "target": "ES6",
    "module": "commonjs",
    "lib": [ 
      "ES6"
    ],
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noImplicitAny": false,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src"
  ]
}
